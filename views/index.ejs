<!DOCTYPE html>
<html>
  <head>
    <title>SCOREBOARD</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>SCOREBOARD</h1>
    <div id="scoreboard">
      <div id="scoreboard-contents">
        <div class="score" id="left">
          <legend><span>HOME</span></legend>
          <div>
            <span class="score_text" id="text_left">0</span>
          </div>
        </div>
        <div id="divider">
          <div class="quad">
          </div>
          <div class="quad">
          </div>
        </div>
        <div class="score" id="right">
          <legend><span>VISITOR</span></legend>
          <div>
            <span class="score_text" id="text_right">0</span>
          </div>
        </div>
      </div>
    </div>
    <script type="text/javascript" src="/javascripts/common.js"></script>
  </body>
</html>
